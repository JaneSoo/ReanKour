<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.janesoo.reankour">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19"/>

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/logoround"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logoround"
        android:supportsRtl="true"
        android:name=".reankour"
        android:theme="@style/AppTheme">


        <activity android:name=".VideoActivity"
            android:configChanges="orientation|screenSize|keyboardHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".ListTutorActivity"></activity>
        <activity android:name=".DetatilActivity"></activity>
        <activity android:name=".DocumentActivity"></activity>
        <activity android:name=".DetailDocumentActivity"></activity>
        <activity android:name=".SignUpActivity"></activity>
        <activity android:name=".AllBottomNavigationViewActivity"
            android:configChanges="orientation|screenSize|keyboardHidden">
        </activity>
        <activity android:name=".StudentProfileActivity"></activity>
        <activity android:name=".TutorProfileActivity"></activity>
        <activity android:name=".ForgotPassActivity"></activity>
        <activity android:name=".EditStudentActivity"></activity>
        <activity android:name=".EditTutorActivity"></activity>


    </application>

</manifest>